    -> FROM profesor p
    -> LEFT JOIN asignatura a ON p.id_profesor = a.id_profesor
    -> GROUP BY p.id_profesor, p.nombre_profesor, p.apellido1_profesor, p.apellido2_profesor
    -> ORDER BY num_asignaturas DESC;
